<%= render partial: "shared/menu"%>
<h1>Welcome Meme Friends! <h1>
<h2> R.i.p Stefan Karl Stefansson</h2>

<% @memes.reverse.each do |m| %> 
    <h4>Name:<%= m.name %></h4></br>
    <%= m.image %></br>
    <h6><%= link_to "#{m.name}'s page", user_meme_path(m[:user_id], m.id) %></h6></br>
    <h6><%= link_to "Owner: #{User.find(m.user_id).username}", user_path(m.user_id) %></h6></br>
    <% end %>